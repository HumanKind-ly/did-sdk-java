{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://localhost:5050/membership-schema.json",
    "type": "object",
    "title": "Membership schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "additionalProperties": true,
    "required": [
        "id",
        "firstName",
        "lastName",
        "membershipLevels"
    ],
    "properties": {
		 "id": {
            "$id": "#/properties/id",
            "type": "string",
            "title": "DID of the owner",
            "default": "",
            "examples": [
                "did:hedera:testnet:DnK9ApZ4GHyAyJY4BL6PKQCJdq1T6jbFpt2bJbRvc63k;hedera:testnet:fid=0.0.1"
            ]
		},
        "firstName": {
            "$id": "#/properties/firstName",
            "type": "string",
            "title": "Owner's first name",
            "default": "",
            "examples": [
                "Jacob"
            ]
        },
        "lastName": {
            "$id": "#/properties/lastName",
            "type": "string",
            "title": "Owner's last name",
            "default": "",
            "examples": [
                "Shepherd"
            ]
        },
        "membershipLevels": {
            "$id": "#/properties/membershipLevels",
            "type": "array",
            "title": "Array of membership levels",
            "default": [],
            "examples": [
                [
                    "Bronze",
                    "Silver",
                    "Gold",
                    "Platinum"
                ]
            ],
            "additionalItems": true,
            "items": {
                "$id": "#/properties/membershipLevels/items",
                "type": "string",
                "title": "A single membership level",
                "default": "",
                "examples": [
                    "Bronze",
                    "Silver",
                    "Gold",
                    "Platinum"
                ]
            }
        }
    }
}